{
  "name": "phenograph_cluster",
  "description": "Cluster for feature extraction data",
  "stage": "1",
  "params": {
    "image": {
      "name": "image",
      "description": "Multichannel image as numpy array",
      "type": "array",
      "hidden": true,
      "required": true
    },
    "all_channels": {
      "name": "all_channels",
      "description": "List of all channels of a image",
      "type": "array",
      "hidden": true,
      "required": true
    },
    "transformation": {
      "name": "transformation",
      "description": "transformation",
      "enum": ["arcsin", "log"],
      "type": "enum",
      "required": true,
      "default": "arcsin"
    },
    "scaling": {
      "name": "scaling",
      "description": "scaling",
      "enum": ["winsorize", "z-score"],
      "type": "enum",
      "required": true,
      "default": "winsorize"
    },
    "knn": {
      "name": "knn",
      "description": "knn",
      "type": "int",
      "required": true,
      "default": 1
    }
  },
  "script_path": "phenograph_cluster",
  "return": {
    "cluster": {
      "name": "adata",
      "type": "array",
      "description": "training transformed"
    }
  },
  "depends_and_script": [
    "feature_extraction"
  ],
  "depends_or_script": ["load_anndata"],
  "libs": [
    "numpy==1.21.0",
    "phenograph==1.5.7",
    "pandas==1.3.4",
    "anndata==0.8.0",
    "scipy==1.9.1",
    "scanpy==1.9.3"
  ]
}
